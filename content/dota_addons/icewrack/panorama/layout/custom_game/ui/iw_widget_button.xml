<root>
	<styles>
		<include src="file://{resources}/styles/custom_game/ui/iw_widget_button.css" />
	</styles>
	
	<scripts>
		<include src="file://{resources}/scripts/custom_game/custom_events.js" />
		<include src="file://{resources}/scripts/custom_game/ui/iw_widget_button.js" />
	</scripts>
	
	<Panel hittest="false" class="SkinnedButtonRoot" onload="OnButtonLoad()">
		<Button class="SkinnedButton" onmouseover="OnButtonMouseOver()" onmouseout="OnButtonMouseOut()" onactivate="OnButtonActivate()">
			<Image id="Texture"/>
			<Image id="TextureOver"/>
			<Image id="TextureDown"/>
			<Image id="TextureDisabled"/>
			<Label class="ButtonText" id="Text" text="" />
		</Button>
	</Panel>
</root>