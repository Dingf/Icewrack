<root>
	<styles>
		<include src="file://{resources}/styles/custom_game/maps/map000/iw_map000.css" />
	</styles>
	
	<scripts>
		<include src="file://{resources}/scripts/custom_game/custom_events.js" />
		<include src="file://{resources}/scripts/custom_game/ui/iw_widget_button.js" />
		<include src="file://{resources}/scripts/custom_game/maps/map000/iw_map000.js" />
	</scripts>
	
	<Panel hittest="false" class="CharacterSelectRoot">
		<Panel hittest="false" class="CharacterSelectHeroGroup" id="HeroGroup">
			<Panel hittest="false" class="CharacterSelectFillerGroup" >
				<Panel hittest="true" class="CharacterSelectFillerTop" />
				<Panel hittest="true" class="CharacterSelectFillerBottom" onactivate="OnCharacterActivate()" oncontextmenu="OnCharacterActivate()"/>
			</Panel>
			<Panel hittest="false" class="CharacterSelectButtonGroup" id="HeroNavGroup" />
			<Panel hittest="false" class="CharacterSelectFlowGroup">
				<Panel hittest="false" class="Spacer64px" />
				<Label hittest="false" class="CharacterSelectTitle" id="HeroTitle" text=""/>
				<Panel hittest="false" class="CharacterSelectFlowGroup" id="HeroDetails">
					<Panel hittest="false" class="CharacterSelectIconContainer" id="HeroIconContainer" />
					<Image hittest="false" class="CharacterSelectDivider" src="file://{images}/custom_game/character_select/iw_character_select_divider.tga"/>
					<Panel hittest="false" class="CharacterSelectIntroContainer">
						<Label hittest="false" class="CharacterSelectIntro" id="HeroText" text="" />
					</Panel>
				</Panel>
			</Panel>
		</Panel>
		<Panel hittest="false" class="CharacterSelectDifficultyGroup" id="DifficultyGroup">
			<Panel hittest="false" class="CharacterSelectFillerGroup" >
				<Panel hittest="true" class="CharacterSelectFillerTop" />
				<Panel hittest="true" class="CharacterSelectFillerBottom" onactivate="OnDifficultyActivate()" oncontextmenu="OnDifficultyActivate()"/>
			</Panel>
			<Panel hittest="false" class="CharacterSelectButtonGroup" id="DifficultyNavGroup" />
			<Panel hittest="false" class="CharacterSelectFlowGroup">
				<Panel hittest="false" class="Spacer64px" />
				<Label hittest="false" class="CharacterSelectTitle" id="DifficultyTitle" text=""/>
				<Panel hittest="false" class="Spacer64px" />
				<Panel hittest="false" class="CharacterSelectDifficultyButtonGroup" id="DifficultyButtonGroup" />
				<Panel hittest="false" class="CharacterSelectFlowGroup" id="DifficultyDetails">
					<Label hittest="false" class="CharacterSelectDifficulty" id="DifficultyText" text=""/>
				</Panel>
			</Panel>
		</Panel>
		<Panel hittest="false" class="CharacterSelectBlackout" id="Blackout"/>
	</Panel>
</root>