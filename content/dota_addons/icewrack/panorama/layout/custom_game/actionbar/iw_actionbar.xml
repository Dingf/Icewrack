<root>
	<styles>
		<include src="file://{resources}/styles/custom_game/actionbar/iw_actionbar.css" />
	</styles>
	
	<scripts>
		<include src="file://{resources}/scripts/custom_game/custom_events.js" />
		<include src="file://{resources}/scripts/custom_game/ui/iw_widget_buff_icon.js" />
		<include src="file://{resources}/scripts/custom_game/actionbar/iw_actionbar_icon.js" />
		<include src="file://{resources}/scripts/custom_game/actionbar/iw_actionbar.js" />
	</scripts>
	
	<Panel hittest="false" class="ActionBarRoot">
		<Panel hittest="false" id="ActionBar">
			<Panel hittest="false" class="ActionBarXPContainer" id="XPContainer"/>
			<Panel hittest="false" class="ActionBarBuffContainer" id="BuffContainer"/>
			<Panel hittest="false" class="ActionBarWindow">
				<Panel hittest="false" class="ActionBarBindContainer" id="BindContainer"/>
				<Panel hittest="false" class="ActionBarIconContainer" id="IconContainer"/>
			</Panel>
			<Panel class="ActionBarTooltip" id="Tooltip" onmouseover="OnTooltipMouseOver()" onmouseout="OnTooltipMouseOut()">
				<Panel class="ActionBarTooltipBody">
					<Panel class="ActionBarTooltipFill" />
					<Panel class="ActionBarTooltipIconContainer" id="TooltipContainer"/>
				</Panel>
				<Image hittest="false" id="TooltipArrow" src="file://{images}/custom_game/actionbar/iw_actionbar_tooltip_arrow.tga"/>
			</Panel>
		</Panel>
	</Panel>
</root>